<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="350.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.LoginController">
   <children>
      <Button fx:id="submitButton" layoutX="159.0" layoutY="219.0" mnemonicParsing="false" onAction="#login" prefHeight="25.0" prefWidth="132.0" text="Submit" />
      <Label layoutX="87.0" layoutY="99.0" text="IP:" />
      <Label layoutX="80.0" layoutY="148.0" text="Port:" />
      <TextField fx:id="ipAddress" layoutX="140.0" layoutY="94.0" promptText="IP" />
      <TextField fx:id="port" layoutX="138.0" layoutY="148.0" promptText="Port" />
      <Label alignment="CENTER" layoutX="192.0" layoutY="26.0" prefHeight="17.0" prefWidth="82.0" text="Login" textFill="#eb0505">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label fx:id="ipCompletion" alignment="CENTER_RIGHT" layoutX="151.0" layoutY="125.0" prefHeight="17.0" prefWidth="149.0" />
      <Label fx:id="portCompletion" alignment="CENTER_RIGHT" layoutX="151.0" layoutY="179.0" prefHeight="17.0" prefWidth="149.0" />
      <Label fx:id="labelStatus" layoutX="161.0" layoutY="291.0" prefHeight="31.0" prefWidth="121.0" />
   </children>
</AnchorPane>
